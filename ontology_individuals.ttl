@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <https://schema.org/> .
@prefix ds: <http://darksouls.ontology/> .

# Ontology Declaration
ds: a owl:Ontology ;
    rdfs:label "Dark Souls Game Ontology" ;
    rdfs:comment "An ontology modeling the mechanics, items, characters, and world of Dark Souls" ;
    owl:versionInfo "1.0" .

# ============================================
# CLASSES (Same as original)
# ============================================

ds:Item a owl:Class ;
    rdfs:label "Item" ;
    rdfs:comment "Any obtainable object in the game world" .

ds:Equipment a owl:Class ;
    rdfs:subClassOf ds:Item ;
    rdfs:label "Equipment" ;
    rdfs:comment "Wearable or usable gear" .

ds:Weapon a owl:Class ;
    rdfs:subClassOf ds:Equipment ;
    rdfs:label "Weapon" ;
    rdfs:comment "Offensive equipment" .

ds:Melee a owl:Class ;
    rdfs:subClassOf ds:Weapon ;
    rdfs:label "Melee Weapon" ;
    rdfs:comment "Close-range weapons" .

ds:Ranged a owl:Class ;
    rdfs:subClassOf ds:Weapon ;
    rdfs:label "Ranged Weapon" ;
    rdfs:comment "Distance weapons" .

ds:Armor a owl:Class ;
    rdfs:subClassOf ds:Equipment ;
    rdfs:label "Armor" ;
    rdfs:comment "Defensive equipment" .

ds:Shield a owl:Class ;
    rdfs:subClassOf ds:Equipment ;
    rdfs:label "Shield" ;
    rdfs:comment "Blocking equipment" .

ds:Ring a owl:Class ;
    rdfs:subClassOf ds:Equipment ;
    rdfs:label "Ring" ;
    rdfs:comment "Accessory equipment" .

ds:Consumable a owl:Class ;
    rdfs:subClassOf ds:Item ;
    rdfs:label "Consumable" ;
    rdfs:comment "Single-use or limited-use items" .

ds:UpgradeMaterial a owl:Class ;
    rdfs:subClassOf ds:Item ;
    rdfs:label "Upgrade Material" ;
    rdfs:comment "Items used to enhance equipment" .

ds:KeyItem a owl:Class ;
    rdfs:subClassOf ds:Item ;
    rdfs:label "Key Item" ;
    rdfs:comment "Quest and progression items" .

ds:BossSoul a owl:Class ;
    rdfs:subClassOf ds:Item ;
    rdfs:label "Boss Soul" ;
    rdfs:comment "Special items dropped by bosses" .

ds:Boss a owl:Class ;
    rdfs:label "Boss" ;
    rdfs:comment "Major enemies with unique souls and mechanics" .

ds:NPC a owl:Class ;
    rdfs:label "NPC" ;
    rdfs:comment "Non-player characters" .

ds:Vendor a owl:Class ;
    rdfs:subClassOf ds:NPC ;
    rdfs:label "Vendor" ;
    rdfs:comment "NPCs who sell items" .

ds:QuestGiver a owl:Class ;
    rdfs:subClassOf ds:NPC ;
    rdfs:label "Quest Giver" ;
    rdfs:comment "NPCs who provide quests" .

ds:Summon a owl:Class ;
    rdfs:subClassOf ds:NPC ;
    rdfs:label "Summon" ;
    rdfs:comment "NPCs who can be summoned for assistance" .

ds:HostileNPC a owl:Class ;
    rdfs:subClassOf ds:NPC ;
    rdfs:label "Hostile NPC" ;
    rdfs:comment "NPCs who attack the player" .

ds:Enemy a owl:Class ;
    rdfs:label "Enemy" ;
    rdfs:comment "Regular hostile creatures" .

ds:Area a owl:Class ;
    rdfs:label "Area" ;
    rdfs:comment "Geographic locations in the game world" .

ds:Covenant a owl:Class ;
    rdfs:label "Covenant" ;
    rdfs:comment "Factions players can join" .

ds:Spell a owl:Class ;
    rdfs:label "Spell" ;
    rdfs:comment "Magic abilities" .

ds:Sorcery a owl:Class ;
    rdfs:subClassOf ds:Spell ;
    rdfs:label "Sorcery" ;
    rdfs:comment "Intelligence-based magic" .

ds:Miracle a owl:Class ;
    rdfs:subClassOf ds:Spell ;
    rdfs:label "Miracle" ;
    rdfs:comment "Faith-based magic" .

ds:Pyromancy a owl:Class ;
    rdfs:subClassOf ds:Spell ;
    rdfs:label "Pyromancy" ;
    rdfs:comment "Fire-based magic" .

ds:UpgradePath a owl:Class ;
    rdfs:label "Upgrade Path" ;
    rdfs:comment "Weapon enhancement routes" .

ds:DamageType a owl:Class ;
    rdfs:label "Damage Type" ;
    rdfs:comment "Categories of damage" .

ds:ArmorSlot a owl:Class ;
    rdfs:label "Armor Slot" ;
    rdfs:comment "Body parts where armor can be equipped" .

# ============================================
# PROPERTIES (Same as original - abbreviated here)
# ============================================

ds:droppedBy a owl:ObjectProperty ;
    rdfs:domain ds:Item ;
    rdfs:range [ a owl:Class ; owl:unionOf (ds:Boss ds:Enemy ds:NPC) ] .

ds:soldBy a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain ds:Item ;
    rdfs:range ds:NPC .

ds:foundIn a owl:ObjectProperty ;
    rdfs:domain ds:Item ;
    rdfs:range ds:Area .

ds:upgradesTo a owl:ObjectProperty, owl:TransitiveProperty ;
    rdfs:domain ds:Weapon ;
    rdfs:range ds:Weapon .

ds:requiresItem a owl:ObjectProperty ;
    rdfs:domain ds:UpgradePath ;
    rdfs:range ds:UpgradeMaterial .

ds:locatedIn a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ; owl:unionOf (ds:NPC ds:Boss ds:Enemy) ] ;
    rdfs:range ds:Area .

ds:connectsTo a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:domain ds:Area ;
    rdfs:range ds:Area .

ds:memberOfCovenant a owl:ObjectProperty ;
    rdfs:domain ds:NPC ;
    rdfs:range ds:Covenant .

ds:weakTo a owl:ObjectProperty ;
    rdfs:domain ds:Boss ;
    rdfs:range ds:DamageType .

ds:resistantTo a owl:ObjectProperty ;
    rdfs:domain ds:Boss ;
    rdfs:range ds:DamageType .

ds:taughtBy a owl:ObjectProperty ;
    rdfs:domain ds:Spell ;
    rdfs:range ds:NPC .

ds:performsUpgrade a owl:ObjectProperty ;
    rdfs:domain ds:NPC ;
    rdfs:range ds:UpgradePath .

ds:drops a owl:ObjectProperty, owl:InverseFunctionalProperty ;
    rdfs:domain ds:Boss ;
    rdfs:range ds:BossSoul .

# Data Properties
ds:soulCost a owl:DatatypeProperty ;
    rdfs:domain ds:Item ;
    rdfs:range xsd:integer .

ds:weight a owl:DatatypeProperty ;
    rdfs:domain ds:Equipment ;
    rdfs:range xsd:decimal .

ds:durability a owl:DatatypeProperty ;
    rdfs:domain ds:Equipment ;
    rdfs:range xsd:integer .

ds:strengthRequirement a owl:DatatypeProperty ;
    rdfs:domain ds:Weapon ;
    rdfs:range xsd:integer .

ds:dexterityRequirement a owl:DatatypeProperty ;
    rdfs:domain ds:Weapon ;
    rdfs:range xsd:integer .

ds:intelligenceRequirement a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ; owl:unionOf (ds:Weapon ds:Spell) ] ;
    rdfs:range xsd:integer .

ds:faithRequirement a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ; owl:unionOf (ds:Weapon ds:Spell) ] ;
    rdfs:range xsd:integer .

ds:physicalDamage a owl:DatatypeProperty ;
    rdfs:domain ds:Weapon ;
    rdfs:range xsd:integer .

ds:magicDamage a owl:DatatypeProperty ;
    rdfs:domain ds:Weapon ;
    rdfs:range xsd:integer .

ds:fireDamage a owl:DatatypeProperty ;
    rdfs:domain ds:Weapon ;
    rdfs:range xsd:integer .

ds:physicalDefense a owl:DatatypeProperty ;
    rdfs:domain ds:Armor ;
    rdfs:range xsd:integer .

ds:magicDefense a owl:DatatypeProperty ;
    rdfs:domain ds:Armor ;
    rdfs:range xsd:integer .

ds:poise a owl:DatatypeProperty ;
    rdfs:domain ds:Armor ;
    rdfs:range xsd:decimal .

ds:stability a owl:DatatypeProperty ;
    rdfs:domain ds:Shield ;
    rdfs:range xsd:integer .

ds:physicalReduction a owl:DatatypeProperty ;
    rdfs:domain ds:Shield ;
    rdfs:range xsd:decimal .

ds:healthPoints a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ; owl:unionOf (ds:Boss ds:Enemy) ] ;
    rdfs:range xsd:integer .

ds:souls a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ; owl:unionOf (ds:Boss ds:Enemy) ] ;
    rdfs:range xsd:integer .

ds:uses a owl:DatatypeProperty ;
    rdfs:domain ds:Spell ;
    rdfs:range xsd:integer .

ds:slots a owl:DatatypeProperty ;
    rdfs:domain ds:Spell ;
    rdfs:range xsd:integer .

# ============================================
# ENUMERATION INDIVIDUALS
# ============================================

# Damage Types
ds:PhysicalDamage a ds:DamageType ;
    rdfs:label "Physical Damage" .

ds:MagicDamage a ds:DamageType ;
    rdfs:label "Magic Damage" .

ds:FireDamage a ds:DamageType ;
    rdfs:label "Fire Damage" .

ds:LightningDamage a ds:DamageType ;
    rdfs:label "Lightning Damage" .

ds:DivineDamage a ds:DamageType ;
    rdfs:label "Divine Damage" .

ds:OccultDamage a ds:DamageType ;
    rdfs:label "Occult Damage" .

# Armor Slots
ds:Head a ds:ArmorSlot ;
    rdfs:label "Head Slot" .

ds:Chest a ds:ArmorSlot ;
    rdfs:label "Chest Slot" .

ds:Hands a ds:ArmorSlot ;
    rdfs:label "Hands Slot" .

ds:Legs a ds:ArmorSlot ;
    rdfs:label "Legs Slot" .

# ============================================
# AREA INDIVIDUALS
# ============================================
# Starting Area
ds:UndeadAsylum a ds:Area ;
    schema:name "Northern Undead Asylum" ;
    schema:description "Starting area and tutorial dungeon" ;
    ds:connectsTo ds:FirelinkShrine .

# Central Hub
ds:FirelinkShrine a ds:Area ;
    schema:name "Firelink Shrine" ;
    schema:description "The central hub of Lordran" ;
    ds:connectsTo ds:UndeadAsylum, ds:UndeadBurg, ds:NewLondoRuins, ds:Catacombs, ds:NewLondoRuins .

# Undead Burg Branch
ds:UndeadBurg a ds:Area ;
    schema:name "Undead Burg" ;
    schema:description "A fortified city overrun by undead" ;
    ds:connectsTo ds:FirelinkShrine, ds:UndeadParish, ds:LowerUndeadBurg, ds:DarkrootBasin .

ds:LowerUndeadBurg a ds:Area ;
    schema:name "Lower Undead Burg" ;
    schema:description "Lower section of the Undead Burg" ;
    ds:connectsTo ds:UndeadBurg, ds:DepthsArea .

ds:DepthsArea a ds:Area ;
    schema:name "The Depths" ;
    schema:description "Flooded sewers beneath the Undead Burg" ;
    ds:connectsTo ds:LowerUndeadBurg, ds:BlighttownUpper .

ds:BlighttownUpper a ds:Area ;
    schema:name "Blighttown (Upper)" ;
    schema:description "Toxic shantytown built into a great chasm" ;
    ds:connectsTo ds:DepthsArea, ds:BlighttownSwamp, ds:ValleyOfDrakes .

ds:BlighttownSwamp a ds:Area ;
    schema:name "Blighttown (Swamp)" ;
    schema:description "Toxic swamp at the base of Blighttown" ;
    ds:connectsTo ds:BlighttownUpper, ds:QuelaagsDomain, ds:GreatHollow .

ds:GreatHollow a ds:Area ;
    schema:name "The Great Hollow" ;
    schema:description "Massive tree descending into the earth" ;
    ds:connectsTo ds:BlighttownSwamp, ds:AshLake .

ds:AshLake a ds:Area ;
    schema:name "Ash Lake" ;
    schema:description "Ancient lake beneath the world with Archtrees" ;
    ds:connectsTo ds:GreatHollow .

# Parish Branch
ds:UndeadParish a ds:Area ;
    schema:name "Undead Parish" ;
    schema:description "A church area housing the Bell of Awakening" ;
    ds:connectsTo ds:UndeadBurg, ds:DarkrootGarden, ds:SensFortress .

ds:DarkrootGarden a ds:Area ;
    schema:name "Darkroot Garden" ;
    schema:description "A mystical forest filled with dangerous creatures" ;
    ds:connectsTo ds:UndeadParish, ds:DarkrootBasin .

ds:DarkrootBasin a ds:Area ;
    schema:name "Darkroot Basin" ;
    schema:description "Lower area of Darkroot with Hydra" ;
    ds:connectsTo ds:DarkrootGarden, ds:UndeadBurg, ds:ValleyOfDrakes .

ds:ValleyOfDrakes a ds:Area ;
    schema:name "Valley of Drakes" ;
    schema:description "Mountain pass inhabited by drakes" ;
    ds:connectsTo ds:DarkrootBasin, ds:BlighttownUpper, ds:NewLondoRuins .

# Sen's Fortress and Anor Londo
ds:SensFortress a ds:Area ;
    schema:name "Sen's Fortress" ;
    schema:description "A deadly fortress filled with traps" ;
    ds:connectsTo ds:UndeadParish, ds:AnorLondo .

ds:AnorLondo a ds:Area ;
    schema:name "Anor Londo" ;
    schema:description "The magnificent city of the gods" ;
    ds:connectsTo ds:SensFortress, ds:PaintedWorld, ds:TheDukesArchives .

ds:PaintedWorld a ds:Area ;
    schema:name "Painted World of Ariamis" ;
    schema:description "Hidden world inside a painting" ;
    ds:connectsTo ds:AnorLondo .

ds:TheDukesArchives a ds:Area ;
    schema:name "The Duke's Archives" ;
    schema:description "Seath's library and research facility" ;
    ds:connectsTo ds:AnorLondo, ds:CrystalCave .

ds:CrystalCave a ds:Area ;
    schema:name "Crystal Cave" ;
    schema:description "A cave of invisible crystal pathways" ;
    ds:connectsTo ds:TheDukesArchives .

# New Londo Branch
ds:NewLondoRuins a ds:Area ;
    schema:name "New Londo Ruins" ;
    schema:description "A flooded city cursed by ghosts" ;
    ds:connectsTo ds:FirelinkShrine, ds:ValleyOfDrakes, ds:TheAbyss .

ds:TheAbyss a ds:Area ;
    schema:name "The Abyss" ;
    schema:description "Void beneath New Londo where Four Kings dwell" ;
    ds:connectsTo ds:NewLondoRuins .

# Catacombs Branch
ds:Catacombs a ds:Area ;
    schema:name "Catacombs" ;
    schema:description "Underground necropolis filled with skeletons" ;
    ds:connectsTo ds:FirelinkShrine, ds:TombOfTheGiants .

ds:TombOfTheGiants a ds:Area ;
    schema:name "Tomb of the Giants" ;
    schema:description "A pitch-black tomb of ancient giants" ;
    ds:connectsTo ds:Catacombs .

# Quelaag's Domain and Demon Ruins
ds:QuelaagsDomain a ds:Area ;
    schema:name "Quelaag's Domain" ;
    schema:description "The lair of the Chaos Witch" ;
    ds:connectsTo ds:BlighttownSwamp, ds:DemonRuins .

ds:DemonRuins a ds:Area ;
    schema:name "Demon Ruins" ;
    schema:description "Ruins of the ancient demons" ;
    ds:connectsTo ds:QuelaagsDomain, ds:LostIzalith .

ds:LostIzalith a ds:Area ;
    schema:name "Lost Izalith" ;
    schema:description "The cursed city consumed by chaos flame" ;
    ds:connectsTo ds:DemonRuins .

# Endgame Area
ds:KilnOfTheFirstFlame a ds:Area ;
    schema:name "Kiln of the First Flame" ;
    schema:description "Final area where Gwyn awaits" ;
    ds:connectsTo ds:FirelinkShrine .

# DLC Areas (Artorias of the Abyss)
ds:RoyalWood a ds:Area ;
    schema:name "Royal Wood" ;
    schema:description "Ancient forest in Oolacile, accessed via DLC" ;
    ds:connectsTo ds:DarkrootBasin, ds:OolacileTownship, ds:Chasm .

ds:OolacileTownship a ds:Area ;
    schema:name "Oolacile Township" ;
    schema:description "Ruins of ancient Oolacile" ;
    ds:connectsTo ds:RoyalWood, ds:OolacileDungeon .

ds:OolacileDungeon a ds:Area ;
    schema:name "Oolacile Dungeon" ;
    schema:description "Underground prison of Oolacile" ;
    ds:connectsTo ds:OolacileTownship, ds:Chasm .

ds:Chasm a ds:Area ;
    schema:name "Chasm of the Abyss" ;
    schema:description "Deepest part of the Abyss where Manus resides" ;
    ds:connectsTo ds:OolacileDungeon, ds:RoyalWood .

